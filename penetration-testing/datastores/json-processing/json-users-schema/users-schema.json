{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Users Schema",
    "type": "object",
    "properties": {
      "users": {
        "type": "array",
        "items": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "id": {
              "type": "integer"
            },
            "username": {
              "type": "string"
            },
            "password": {
              "type": "string"
            }
          },
          "required": ["id", "username", "password"]
        }
      }
    },
    "required": ["users"]
  }
  